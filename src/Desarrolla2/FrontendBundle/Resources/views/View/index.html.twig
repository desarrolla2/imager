{% extends 'FrontendBundle::layout_search.html.twig' %}
{% block title %} {{ image.name | upper }} | {{ parent() }}{% endblock %}
{% block body %}
    <div id="image-result">     
        {% if image %}
            {% include "FrontendBundle:View:item.html.twig" %}
        {% else %}
            <h1>Opps!!</h1>
            <p>We cant found image, you can try with other search terms!</p>
        {% endif %}
    </div>
{% endblock %}